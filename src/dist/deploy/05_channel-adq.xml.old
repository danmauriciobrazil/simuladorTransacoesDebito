<?xml version="1.0" encoding="UTF-8"?>
<channel-adaptor name="adq-channel"
				 class="org.jpos.q2.iso.ChannelAdaptor"
				 realm="channel-adaptor-visa-primary"
				 logger="Q2">

	<channel class="org.jpos.iso.channel.NACChannel"
			 name="adq-channel"
			 realm="adq-channel"
			 packager="org.jpos.iso.packager.GenericPackager"
			 logger="Q2"
			 type="client">

		<property name="host" value="192.168.7.222"/>
		<property name="port" value="10010"/>
		<property name="timeout" value="360000"/>
		<property name="packager-config" value="cfg/packager.xml"/>
	</channel>

	<in>adq-channel-send</in>
	<out>adq-channel-receive</out>
	<reconnect-delay>1000</reconnect-delay>
</channel-adaptor>